    "import datetime\n",
    "\n",
    "start=datetime.datetime(1901,1,1,8)\n",
    "td = datetime.timedelta(days=1)\n",
    "\n",
    "\n",
    "curtime=start\n",
    "dsum=0\n",
    "while curtime.year<2001:\n",
    "    if curtime.weekday()==6 and curtime.day==1:\n",
    "        #print(curtime)\n",
    "        dsum+=1\n",
    "    curtime+=td\n",
    "dsum"
   ]
  }
